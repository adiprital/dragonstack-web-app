:"components/PublicDragonRow.js","original":{"line":37,"column":8}},{"generated":{"line":118,"column":4},"source":"components/PublicDragonRow.js","original":{"line":56,"column":5}},{"generated":{"line":122,"column":2},"source":"components/PublicDragonRow.js","original":{"line":8,"column":30},"name":"Component"},{"generated":{"line":124,"column":15},"source":"components/PublicDragonRow.js","original":{"line":59,"column":15},"name":"PublicDragonRow"}],"sources":{"components/PublicDragonRow.js":"import React, { Component } from 'react';\r\nimport { Button } from 'react-bootstrap';\r\nimport DragonAvatar from './DragonAvatar';\r\nimport MatingOptions from './MatingOptions';\r\nimport { BACKEND } from '../config';\r\nimport history from '../history';\r\n\r\nclass PublicDragonRow extends Component {\r\n    state = { displayMatingOptions: false };\r\n\r\n    toggleDisplayMatingOptions = () => {\r\n        this.setState({\r\n            displayMatingOptions: !this.state.displayMatingOptions\r\n        });\r\n    }\r\n\r\n    buy = () => {\r\n       const { dragonId, saleValue } = this.props.dragon;\r\n       \r\n       fetch(`${BACKEND.ADDRESS}/dragon/buy`, {\r\n           method: 'POST',\r\n           credentials: 'include',\r\n           headers: { 'Content-Type': 'application/json' },\r\n           body: JSON.stringify({ dragonId, saleValue })\r\n       }) .then(response => response.json())\r\n          .then(json => {\r\n              alert(json.message);\r\n\r\n              if (json.type !== 'error') {\r\n                history.push('/account-dragons');\r\n              }\r\n          })\r\n          .catch(error => alert(error.message));\r\n    }\r\n    \r\n    render() {\r\n        return (\r\n            <div>\r\n                <div>{this.props.dragon.nickname}</div>\r\n                <DragonAvatar dragon={this.props.dragon} />\r\n                <div>\r\n                    <span>Sale Value: {this.props.dragon.saleValue}</span>{' | '}\r\n                    <span>Sire Value: {this.props.dragon.sireValue}</span>\r\n                </div>\r\n                <br />\r\n                <Button onClick={this.buy}>Buy</Button>{' '}\r\n                <Button onClick={this.toggleDisplayMatingOptions}>Sire</Button>\r\n                <br />\r\n                {\r\n                    this.state.displayMatingOptions ?\r\n                     <MatingOptions patronDragonId={this.props.dragon.dragonId} /> :\r\n                     <div></div>\r\n                }\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default PublicDragonRow;"},"lineCount":null}},"error":null,"hash":"b6688f0c6a70730ba7ffbd8ebcbc7b83","cacheData":{"env":{}}}